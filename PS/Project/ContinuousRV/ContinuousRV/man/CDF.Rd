\name{CDF}
\alias{CDF}

\title{
  Cumulative distribution function
}
\description{
  Calculates the cumulative distribution for a given CRV, up to x.
}
\usage{
  CDF(crv, x)
}

\arguments{
  \item{crv}{
    A CRV object
  }
  \item{x}{
    The upper bound of the cumulative distribution
  }
}

\value{
  A numeric value
}

\examples{
  # Create a new CRV object
  crv <- CRV(function(x) {x}, 0, 1)

  # Returns 0.25
  CDF(crv, 0.5)
}
